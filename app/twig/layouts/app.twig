<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport"
				content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<title>Mazda portal</title>

	<!-- Stylesheets -->
	{% include "stylesheets.twig" %}

	{% block header_scripts %}{% endblock %}
</head>
<body>
<div class="site-wrap">

	{% if cleanPage != "true" %}
	<!-- Header block -->
	{% include "header.twig" %}
	<!-- Header block end -->
	{% endif %}

	<!-- Block content -->
	{% block content %}{% endblock %}
	<!-- Block content end -->

	{% if cleanPage != "true" %}
	<!-- Footer block -->
	{% include "footer.twig" %}
	<!-- Footer block end -->
	{% endif %}
</div>

<!-- Scripts -->
{% include "scripts.twig" %}
</body>
</html>